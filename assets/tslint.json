{
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-react-hooks"
    ],
    "rules": {
        "interface-name": [true, "never-prefix"],
        "quotemark": [true, "single", "jsx-double", "avoid-escape"],
        "jsx-curly-spacing": [true, "always"],
        "jsx-boolean-value": [true, "never"],
        "no-implicit-dependencies": [true, "dev"]
    }
}
